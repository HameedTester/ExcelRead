package swathi;

public class Arithmatic {

int var1 = 10;
int var2 = 5;

public void add () {
	
	
	System.out.println(var1 + var2);//15
	
	
}
public void subtraction () {
	
	System.out.println(var1 - var2);//5
	
}
public static void main(String[] args) {
	
	Arithmatic a = new Arithmatic ();
	a .add ();
	
	
	
}
	
	
	
	
	
	
	
	
	
	
	
	
	
}
