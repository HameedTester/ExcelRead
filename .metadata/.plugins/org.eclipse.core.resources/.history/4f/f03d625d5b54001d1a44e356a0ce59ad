package inheritanceDemo;

public class SingleDimensionalArray {

	int a[]= {12,54,67,34,76,98,23,12};


	public void arrayDemo()
	{


		for(int i=0; i<a.length;i++) //   0 ; 0<8 , 1<8  // Selective
		{

			System.out.println(a[i]); //a[0]-->12, a[1]-->54
		}

		// Enhanced For Loop // Cant able to select the condition// 


		for(int k:a)
		{
			System.out.println(k); //12 54

		}

		for(int i=a.length-1; i>=0; i--) // 8-1; 7>=0
		{

			System.out.print(a[i] + " ");
		}


	}
	public static void main(String[] args) {

		SingleDimensionalArray s = new SingleDimensionalArray();
		s.arrayDemo();

	}

}
