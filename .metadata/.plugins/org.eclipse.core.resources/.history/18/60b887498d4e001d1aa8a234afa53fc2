package nestedLoop;

public class PrintStars {
	
	
	public void printStars()
	{
		
		for(int row=1; row<=5; row++) // 1<=5 ,2<=5
		{
			
			for(int col=1; col<=5; col++) //1<=5 
			{
				
			
				if(row ==1 || row==5 || col==1 || col==5) //1==5
				{
					
					System.out.println("* ");
					
				}
				else
				{
					
					System.out.println("  ");
					
				}
				
				
			}
			System.out.println();
		}
		
		
	}

	public static void main(String[] args) {
		
		PrintStars p = new PrintStars();
		p.printStars();
	}
}
