package twoDimensional;

public class TwoDimensional {


	int a[][] = {

			{23,45,65,78},// -- rows
			{34,56,77,90},
			{21,54,56,87},
			// columns

	};
	public void arrayDemo()
	{
		
		System.out.println(a[0][0]); //23
		System.out.println(a[2][3]);

		
		

	}

	public static void main(String[] args) {
		TwoDimensional t = new TwoDimensional();
		t.arrayDemo();
	}



}
