package starPattern;

import java.util.Scanner;

public class ScannerStringDemo {


	String s1; //nextLine()// nextInt()

	String s2;

	public void scannerString()
	{

		Scanner sc = new Scanner(System.in);
		s1 = sc.nextLine();

		s2 = sc.nextLine();

		System.out.println("Concatinating Two Strings:"+ (s1 + s2));
		sc.close();

	}

	public static void main(String[] args) {

		ScannerStringDemo s = new ScannerStringDemo();
		s.scannerString();

	}
}
