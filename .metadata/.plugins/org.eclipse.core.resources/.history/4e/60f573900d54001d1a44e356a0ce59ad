package inheritanceDemo;

public class ArrayDemoProgram {


	int arr[] = {23,24,54,65,78,67,34,23,98};  // length


	public void arrayDemo()
	{


		// Reverse an Array
		
		for(int i=arr.length; i>=0; i--) // 98
			
		{
			
			System.out.println(arr[i]); // 98, 23,
		}

	}


	public static void main(String[] args) {

		ArrayDemoProgram a = new ArrayDemoProgram();
		a.arrayDemo();

	}
}
