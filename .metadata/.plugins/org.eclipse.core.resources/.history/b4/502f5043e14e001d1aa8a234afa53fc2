package starPattern;

public class MethodsInJava {
	int a=10;
	int b=20;
	
	
	public void addNumbers()
	{
		
		
	}

	
	public void subNumbers(int a, int b)
	{
		
		
	}
	
	public returnType mulNumbers()
	{
		
		
	}
	
	public returnType divNumbers(int a, int b)
	{
		
		
		
	}

public static void main(String[] args) {
	
	MethodsInJava m = new MethodsInJava();
	m.addNumbers();
	m.subNumbers(20, 30);
	m.mulNumbers();
	m.divNumbers(40, 20);
	
}
}

