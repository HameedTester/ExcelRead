package arrayDemo;

public class TwoDimensionalDemoArray {
	
	int a[][] = {
			
			
			{12,23,45},
			{23,54,65},
			{89,56,89,45,67,89,90},
			
			
	};
	
	
	public void twoDimensional()
	{
		
//		System.out.println(a.length);
//		System.out.println(a[0][0]);
//		System.out.println(a[2][1]);
//		System.out.println(a[2].length);
		
		
		for(int row=0; row<3; row++) // 0--> 0<3
		{
			
			
			for(int col=0; col<a[row].length; col++) // 0--> 0<3 3<3
			{
				
				
				System.out.print(a[row][col]+" "); //a[0][0] //  12
				
				
			}
			System.out.println();
			
		}
		
		
	}
	
	public static void main(String[] args)
	{
		
		
		TwoDimensionalDemoArray t = new TwoDimensionalDemoArray();
		t.twoDimensional();
		
		
	}

}
