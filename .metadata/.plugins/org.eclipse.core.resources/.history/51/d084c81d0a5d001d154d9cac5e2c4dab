package swathi;

public class StringExampleDemo {
	
	String s = "Java 7PM Batch Chrompet";
	
	
	public void stringProgram()
	{
		
		// length
		
		for(int i=s.length()-1;i>=0;i--)
		{
			
			System.out.print(i);
		}
		
		
	}

	public static void main(String[] args) {
		
		StringExampleDemo s = new StringExampleDemo();
		s.stringProgram();
	}
}
