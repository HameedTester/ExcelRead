package loopsDemo;

import java.util.Scanner;

public class ScannerClassDemo {
	
	int var1;
	int var2;
	
	public void scannerDemoClass()
	{
		
		Scanner s = new Scanner(System.in);
		int var1 = s.nextInt();
		int var2 = s.nextInt();
		System.out.println(var1+var2);
		s.close();
		
		
		
	}
	
	public static void main(String[] args) {
		
		ScannerClassDemo s = new ScannerClassDemo();
		s.scannerDemoClass();
	}

}
