package loopsDemo;

public class NestedLoopProgram {


	public void nestedLoop()
	{

		for(int row=1; row<=5; row++)
		{

			for(int col=1; col<=5; col++)

			{

				if(row%2==1 && col%==0 || row%2==0 && col%2==0)
				{
					
					System.out.print("* ");
					
				}
				
				else
				{
					
					System.out.print("  ");
				}

			}
			System.out.println();

			


		}


	}

	public static void main(String[] args) {

		NestedLoopProgram n = new NestedLoopProgram();
		n.nestedLoop();
	}

}
