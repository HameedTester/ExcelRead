package inheritanceDemo;

public class SonHouse {
	
	public void ktmBike()
	{
		
		System.out.println("Sons KTM Bike");
		
	}
	
	public void pursarBike()
	{
		
		System.out.println("Sons Pulsar Bike");
	}
	
	
	public static void main(String[] args)
	{
		
		
		SonHouse s = new SonHouse();
		s.ktmBike();
		s.pursarBike();
		
		
	}

}
