package starPattern;

public class MethodsInJava {
	int a=10;
	int b=20;
	
	public void methodName()
	{
		
	System.out.println(a);	
	}

	
	public void methodName(int a, int b) // Local Variable
	{
		System.out.println(a+b);
		
	}
	
	public int subNumber()
	{
		
		return a-b;
		
	}
	
	
	public int mulNumbers(int a, int b)
	{
		
		return a*b;
		
	}
	
	
	public static void main(String[] args) {
		MethodsInJava m = new MethodsInJava();
		m.methodName();
		m.methodName(3, 2);
		m.subNumber();
		m.mulNumbers(20, 40);
	}
}
