package oneDimensionalArray;

public class OneDimensionalArray {

	int a[] = {2,6,4,8,9,10,34};// index bassed --> length is 7, index--> 6


	public void arrayDemo()
	{

		for(int i=0; i<3; i++) // 0<6 , 1<6  // Selective Loop // Ctrl + ?
		{

			System.out.println(a[i]); //a[0]->, a[1]-->6
		}


		// Enhanced For loop


		for (int i : a) {
			System.out.println(i);

		}


	}

	public static void main(String[] args) {

		OneDimensionalArray o = new OneDimensionalArray();
		o.arrayDemo();

	}
}
