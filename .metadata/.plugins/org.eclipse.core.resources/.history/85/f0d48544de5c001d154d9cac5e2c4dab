package swathi;

public class ArrayConcept {

int var1 =10;
int arr []= {12,45,89,56,78};

public void array () {
	
	System.out.println(var1);//10
	System.out.println(arr);//address
	System.out.println(arr[1]);//45
	
	
	
	
}
public static void main(String[] args) {

ArrayConcept a = new ArrayConcept ();
a .array ();
	
	
	
	
}

	
	
	
	
	
	
	
	
	
	
	
	
	
}
