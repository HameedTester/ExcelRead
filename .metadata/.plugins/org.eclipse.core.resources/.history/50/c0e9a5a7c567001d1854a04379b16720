package dataType;

public class LadderControlStatements {
	
	int num =24;
	
	
	public void ladderStatements()
	{
		if (condition1)--> var1 =10, var2=20 --> var1>var--> 10>20
				{
				statements1;
				}
				else if (condition2)
				        {
				        statements2;
				        }
				        else if (condition3)
				      	  {
				       	 statements3;
				        	  }
				  	 else if (condition4)
				      	       {
				       	       statements4;
				        	       }

					       else
					       {
				                          statements;
					         }

		
		
	}
	
	public static void main(String[] args)
	{
		
		LadderControlStatements l = new LadderControlStatements();
		l.ladderStatements();
		
		
		
	}

}
