package starPattern;

public class StarPatternProgram {




	public void starPatter()
	{

		for(int row=1; row<=5; row++) // 1<=5
		{

			for(int col=1; col<=5; col++) // 1<=5

			{

				System.out.print("* "); //

			}


		}



	}

	public static void main(String[] args) {

		StarPatternProgram s = new StarPatternProgram();
		s.starPatter();
	}

}
