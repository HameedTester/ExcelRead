package loopsDemo;

public class NestedLoopProgram {


	public void nestedLoop()
	{

		for(int row=1; row<=5; row++)
		{

			for(int col=1; col<=5; col++)

			{

				System.out.print("* ");

			}

			System.out.println();


		}


	}

	public static void main(String[] args) {

		NestedLoopProgram n = new NestedLoopProgram();
		n.nestedLoop();
	}

}
