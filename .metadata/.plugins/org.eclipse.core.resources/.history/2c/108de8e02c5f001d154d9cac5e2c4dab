package swathi;

public class ControlStatement {

int num =24;

public void evencheck () {
	if(num%2==0)
	{
		System.out.println(num+"is a completely divided by 2");
	}
	
}
public static void main(String[] args) {
ControlStatement c	 = new ControlStatement ();
c .evencheck ();
	
	
	
	
}


	
	
	
	
	
	
	
	
	
	
}
