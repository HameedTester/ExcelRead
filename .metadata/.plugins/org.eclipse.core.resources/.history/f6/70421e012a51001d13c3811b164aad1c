package inheritanceDemo;

public class SonHouse extends FatherHouse { // Single Inheritance
	
	
	public void ktmBike()
	{
		System.out.println("Sons KTM Bike");
		
	}

	
	public void pulsarBike()
	{
		
		
		System.out.println("Sons Pulsar Bike");
	}
	
	
	public static void main(String[] args) {
		
		
		SonHouse s = new SonHouse();
		s.ktmBike(); // sons own property
		s.pulsarBike(); // sons own property
		s.audiCar(); // Fathers Property
		s.porscheCar(); // Fathers Property
		s.biCycle(); // GrandFather Property
		s.heritageHome(); // GrandFather Property
		
			
		
	}
	
}
