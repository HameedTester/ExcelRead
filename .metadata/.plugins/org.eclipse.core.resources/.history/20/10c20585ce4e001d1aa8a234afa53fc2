package starPattern;

import java.util.Scanner;

public class ScannerClassDemo {
	
	
	int a;
	
	int b;
	
	
	public void addNumbers()
	{
		
	Scanner sc = new Scanner(System.in); // 	
	a = sc.nextInt();
	b = sc.nextInt();
	
	System.out.println("Addition of Two Numbers:"+(a%b)); // sub, mul, div
	
	sc.close();
		
	}
	
	public static void main(String[] args) {
		
		ScannerClassDemo s = new ScannerClassDemo();
		s.addNumbers();
		
	}

}
