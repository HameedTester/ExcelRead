package inheritanceDemo;

public class ChildCalculator extends FatherCalc {

	public void modNumbers()
	{


		System.out.println("Modulation of Two Numbers");
	}

	public void divNumbers()
	{

		System.out.println("Division of Two Numbers");
	}
	
	public void mulNumbers()
	{
		
		System.out.println("Multiplication of Two Numbers");
	}
	
	
	public static void main(String[] args) {
		ChildCalculator c = new ChildCalculator();
		c.addNumbers();// father calc
		c.subNumbers(); // father calc
		c.mulNumbers(); // father calc
		c.divNumbers(); // child calc
		c.modNumbers();// child calc
	}

}
