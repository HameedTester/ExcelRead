package snehaDemo;

public class FirstProgram {

	int var1 = 10;
	int var2 = 5;
	
	
	public void add () {
		
		
		System.out.println(var1 + var2);//15
		
		
		
	}
	public void sub () {
		
		
		System.out.println(var1 - var2);//5
		
	}
	public void multi () {
		
		
		
		System.out.println(var1 * var2);//50
	}
	public void divide () {
		
		System.out.println(var1 / var2);//2
	}
	
	public static void main(String[] args) {
		
		FirstProgram f = new FirstProgram ();
		f.add ();
		f.sub ();
		f.multi ();
		
	}
	
	
	
	
	
}
