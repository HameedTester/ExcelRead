package dataType;

public class SwitchCaseControl {


	String  proof ="Pan";


	public void switchDemo()
	{


		//switch case syntax

		switch(proof)
		{

		case "Aadhar":
			System.out.println("This is Aadhar Car");
			break;

		case "Passport":

			System.out.println("This is Passport");
			break;

		case "Vote ID":
			System.out.println("This is Voter ID");
			break;

		case "Pan":
			System.out.println("This is My Pan Card");
			break;


		default:
			System.out.println("No Matching Proofs are Found");
			break;







		}




	}



	public static void main(String[] args)
	{

		SwitchCaseControl s = new SwitchCaseControl();
		s.switchDemo();



	}

}
